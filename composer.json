{
    "name": "mtarld/json-encoder-bench",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {"type": "path", "url": "../symfony/src/Symfony/Component/Encoder/"},
        {"type": "path", "url": "../symfony/src/Symfony/Component/Json/"},
        {"type": "path", "url": "../symfony/src/Symfony/Component/TypeInfo/"}
    ],
    "require": {
        "symfony/serializer": "*",
        "symfony/property-access": "*",
        "symfony/console": "^6.3",
        "symfony/json": "*",
        "symfony/cache": "7.0.x-dev",
        "phpdocumentor/reflection-docblock": "^5.3"
    },
    "require-dev": {
        "phpbench/phpbench": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    }
}
